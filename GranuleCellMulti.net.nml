        </gate>
        <gate id="h" instances="1" type="gateHHrates">
            <notes>Note: offset from ChannelML file incorporated into midpoint of rates!!</notes>
            <q10Settings experimentalTemp="17.350264793 degC" type="q10ExpTemp" q10Factor="3"/>
            <forwardRate type="Gran_KDr_98_h_alpha_rate"/>
            <reverseRate type="Gran_KDr_98_h_beta_rate"/>
        </gate>
    </ionChannel>
    <ionChannel id="Gran_NaF_98" conductance="10pS" type="ionChannelHH" species="na">
        <notes>Fast inactivating Na+ channel</notes>
        <annotation/>
        <gate id="m" instances="3" type="gateHHratesTau">
            <notes>Note: offset from ChannelML file incorporated into midpoint of rates!!</notes>
            <q10Settings experimentalTemp="17.350264793 degC" type="q10ExpTemp" q10Factor="3"/>
            <forwardRate midpoint="-0.028999999999999998V" rate="1500per_s" scale="0.012345679V" type="HHExpRate"/>
            <reverseRate midpoint="-0.028999999999999998V" rate="1500per_s" scale="-0.0151515V" type="HHExpRate"/>
            <timeCourse type="Gran_NaF_98_m_tau_tau"/>
        </gate>
        <gate id="h" instances="1" type="gateHHratesTau">
            <notes>Note: offset from ChannelML file incorporated into midpoint of rates!!</notes>
            <q10Settings experimentalTemp="17.350264793 degC" type="q10ExpTemp" q10Factor="3"/>
            <forwardRate midpoint="-0.04V" rate="120per_s" scale="-0.01123596V" type="HHExpRate"/>
            <reverseRate midpoint="-0.04V" rate="120per_s" scale="0.01123596V" type="HHExpRate"/>
            <timeCourse type="Gran_NaF_98_h_tau_tau"/>
        </gate>
    </ionChannel>
    <ionChannel id="GranPassiveCond" conductance="10pS" type="ionChannelPassive">
        <notes>Simple leak conductance for Granule cell</notes>
        <annotation/>
    </ionChannel>
    <decayingPoolConcentrationModel id="Gran_CaPool_98" ion="ca" shellThickness="8.4e-8m" restingConc="7.55e-5mM" decayConstant="1e-2s"/>
    <cell neuroLexId="nifext_128" id="Granule_98">
        <notes>An implementation using ChannelML of the Granule cell mode from Maex, R and De Schutter, E. Synchronization of Golgi and Granule Cell Firing in a Detailed Network Model of the Cerebellar Granule Cell Layer, 1998</notes>
        <morphology id="morphology_Granule_98">
            <segment id="0" name="Soma">
                <proximal y="0.000000e+00" x="0.000000e+00" z="0.000000e+00" diameter="1.000000e+01"/>
                <distal y="0.000000e+00" x="0.000000e+00" z="0.000000e+00" diameter="1.000000e+01"/>
            </segment>
            <segmentGroup neuroLexId="sao864921383" id="Soma">
                <member segment="0"/>
            </segmentGroup>
            <segmentGroup id="all">
                <include segmentGroup="Soma"/>
            </segmentGroup>
            <segmentGroup neuroLexId="GO:0043025" id="soma_group">
                <include segmentGroup="Soma"/>
            </segmentGroup>
        </morphology>
        <biophysicalProperties id="biophys">
