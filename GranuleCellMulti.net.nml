                <channelDensity id="Gran_CaHVA_98_all" segmentGroup="all" ion="ca" ionChannel="Gran_CaHVA_98" erev="80.0 mV" condDensity="0.9084216 mS_per_cm2"/>
                <channelDensity id="Gran_H_98_all" segmentGroup="all" ion="h" ionChannel="Gran_H_98" erev="-42.0 mV" condDensity="0.03090506 mS_per_cm2"/>
                <channelDensity id="Gran_KA_98_all" segmentGroup="all" ion="k" ionChannel="Gran_KA_98" erev="-90.0 mV" condDensity="1.14567 mS_per_cm2"/>
                <channelDensity id="Gran_KCa_98_all" segmentGroup="all" ion="k" ionChannel="Gran_KCa_98" erev="-90.0 mV" condDensity="17.9811 mS_per_cm2"/>
                <channelDensity id="Gran_KDr_98_all" segmentGroup="all" ion="k" ionChannel="Gran_KDr_98" erev="-90.0 mV" condDensity="8.89691 mS_per_cm2"/>
                <channelDensity id="Gran_NaF_98_all" segmentGroup="all" ion="na" ionChannel="Gran_NaF_98" erev="55.0 mV" condDensity="55.7227 mS_per_cm2"/>
                <channelDensity id="GranPassiveCond_all" segmentGroup="all" ion="non_specific" ionChannel="GranPassiveCond" erev="-65.0 mV" condDensity="0.0330033 mS_per_cm2"/>
                <spikeThresh segmentGroup="all" value="0 mV"/>
