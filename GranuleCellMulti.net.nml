    <ComponentType extends="baseVoltageDepRate" name="Gran_CaHVA_98_h_alpha_rate">
        <Constant name="TIME_SCALE" value="1 s" dimension="time"/>
        <Constant name="VOLT_SCALE" value="1 V" dimension="voltage"/>
        <Constant name="offset" value="0.010V" dimension="voltage"/>
        <Dynamics>
            <DerivedVariable name="V" value="(v - offset) / VOLT_SCALE" dimension="none"/>
            <ConditionalDerivedVariable exposure="r" name="r" dimension="per_time">
                <Case value="( 5.0 ) / TIME_SCALE" condition="V   .lt. ( -0.060 )"/>
                <Case value="( 5 * (exp (-50 * (V - (-0.060))))) / TIME_SCALE"/>
            </ConditionalDerivedVariable>
        </Dynamics>
    </ComponentType>
    <ComponentType extends="baseVoltageDepRate" name="Gran_CaHVA_98_h_beta_rate">
        <Constant name="TIME_SCALE" value="1 s" dimension="time"/>
        <Constant name="VOLT_SCALE" value="1 V" dimension="voltage"/>
        <Constant name="offset" value="0.010V" dimension="voltage"/>
        <Dynamics>
            <DerivedVariable name="V" value="(v - offset) / VOLT_SCALE" dimension="none"/>
            <ConditionalDerivedVariable exposure="r" name="r" dimension="per_time">
                <Case value="( 0 ) / TIME_SCALE" condition="V   .lt. ( -0.060 )"/>
                <Case value="( 5 - (5 * (exp (-50 * (V - (-0.060)))))) / TIME_SCALE"/>
            </ConditionalDerivedVariable>
        </Dynamics>
    </ComponentType>
    <ComponentType extends="baseVoltageDepTime" name="Gran_KA_98_m_tau_tau">
        <Constant name="TIME_SCALE" value="1 s" dimension="time"/>
        <Constant name="VOLT_SCALE" value="1 V" dimension="voltage"/>
        <Constant name="offset" value="0.010V" dimension="voltage"/>
        <Dynamics>
            <DerivedVariable name="V" value="(v - offset) / VOLT_SCALE" dimension="none"/>
            <DerivedVariable exposure="t" name="t" value="(0.410e-3 * ((exp (( ((V) + 0.0435) / (-0.0428))))) + 0.167e-3) * TIME_SCALE" dimension="time"/>
        </Dynamics>
    </ComponentType>
    <ComponentType extends="baseVoltageDepTime" name="Gran_KA_98_h_tau_tau">
        <Constant name="TIME_SCALE" value="1 s" dimension="time"/>
        <Constant name="VOLT_SCALE" value="1 V" dimension="voltage"/>
        <Constant name="offset" value="0.010V" dimension="voltage"/>
        <Dynamics>
            <DerivedVariable name="V" value="(v - offset) / VOLT_SCALE" dimension="none"/>
            <DerivedVariable exposure="t" name="t" value="(0.001 * (10.8 + (30 * V) + (1 / (57.9 * (exp (V * 127)) + (134e-6 * (exp (V * (-59)))))))) * TIME_SCALE" dimension="time"/>
        </Dynamics>
    </ComponentType>
    <ComponentType extends="baseVoltageConcDepRate" name="Gran_KCa_98_m_alpha_rate">
        <Constant name="TIME_SCALE" value="1 s" dimension="time"/>
        <Constant name="VOLT_SCALE" value="1 V" dimension="voltage"/>
        <Constant name="CONC_SCALE" value="1 mM" dimension="concentration"/>
        <Constant name="offset" value="0.010V" dimension="voltage"/>
        <Dynamics>
            <DerivedVariable name="V" value="(v - offset) / VOLT_SCALE" dimension="none"/>
            <DerivedVariable name="ca_conc" value="caConc / CONC_SCALE" dimension="none"/>
            <DerivedVariable exposure="r" name="r" value="(2500/(1 + ( (1.5e-3 *(exp (-85*V))) / ca_conc))) / TIME_SCALE" dimension="per_time"/>
        </Dynamics>
    </ComponentType>
    <ComponentType extends="baseVoltageConcDepRate" name="Gran_KCa_98_m_beta_rate">
        <Constant name="TIME_SCALE" value="1 s" dimension="time"/>
        <Constant name="VOLT_SCALE" value="1 V" dimension="voltage"/>
        <Constant name="CONC_SCALE" value="1 mM" dimension="concentration"/>
        <Constant name="offset" value="0.010V" dimension="voltage"/>
        <Dynamics>
            <DerivedVariable name="V" value="(v - offset) / VOLT_SCALE" dimension="none"/>
            <DerivedVariable name="ca_conc" value="caConc / CONC_SCALE" dimension="none"/>
            <DerivedVariable exposure="r" name="r" value="(1500/(1 + (ca_conc / (1.5e-4 * (exp (-77*V)))))) / TIME_SCALE" dimension="per_time"/>
        </Dynamics>
    </ComponentType>
    <ComponentType extends="baseVoltageDepRate" name="Gran_KDr_98_m_alpha_rate">
        <Constant name="TIME_SCALE" value="1 s" dimension="time"/>
        <Constant name="VOLT_SCALE" value="1 V" dimension="voltage"/>
        <Constant name="offset" value="0.010V" dimension="voltage"/>
        <Dynamics>
            <DerivedVariable name="V" value="(v - offset) / VOLT_SCALE" dimension="none"/>
            <DerivedVariable exposure="r" name="r" value="(170 * ((exp (73 *(V - (-0.038)))))) / TIME_SCALE" dimension="per_time"/>
        </Dynamics>
    </ComponentType>
    <ComponentType extends="baseVoltageDepRate" name="Gran_KDr_98_m_beta_rate">
        <Constant name="TIME_SCALE" value="1 s" dimension="time"/>
        <Constant name="VOLT_SCALE" value="1 V" dimension="voltage"/>
        <Constant name="offset" value="0.010V" dimension="voltage"/>
        <Dynamics>
            <DerivedVariable name="V" value="(v - offset) / VOLT_SCALE" dimension="none"/>
            <DerivedVariable exposure="r" name="r" value="(170 * ((exp ((-18) *(V - (-0.038)))))) / TIME_SCALE" dimension="per_time"/>
        </Dynamics>
    </ComponentType>
    <ComponentType extends="baseVoltageDepRate" name="Gran_KDr_98_h_alpha_rate">
        <Constant name="TIME_SCALE" value="1 s" dimension="time"/>
        <Constant name="VOLT_SCALE" value="1 V" dimension="voltage"/>
        <Constant name="offset" value="0.010V" dimension="voltage"/>
        <Dynamics>
            <DerivedVariable name="V" value="(v - offset) / VOLT_SCALE" dimension="none"/>
            <ConditionalDerivedVariable exposure="r" name="r" dimension="per_time">
                <Case value="( 0.76 ) / TIME_SCALE" condition="V  .gt. ( -0.046 )"/>
                <Case value="( 0.7 + 0.065*(exp (-80*(V - (-0.046))))) / TIME_SCALE"/>
            </ConditionalDerivedVariable>
        </Dynamics>
    </ComponentType>
    <ComponentType extends="baseVoltageDepRate" name="Gran_KDr_98_h_beta_rate">
        <Constant name="TIME_SCALE" value="1 s" dimension="time"/>
        <Constant name="VOLT_SCALE" value="1 V" dimension="voltage"/>
        <Constant name="offset" value="0.010V" dimension="voltage"/>
        <Dynamics>
            <DerivedVariable name="V" value="(v - offset) / VOLT_SCALE" dimension="none"/>
            <DerivedVariable exposure="r" name="r" value="(1.1/(1 + (exp (-80.7 * (V - (-0.044)))))) / TIME_SCALE" dimension="per_time"/>
        </Dynamics>
    </ComponentType>
    <ComponentType extends="baseVoltageDepTime" name="Gran_NaF_98_m_tau_tau">
        <Constant name="TIME_SCALE" value="1 s" dimension="time"/>
        <Constant name="VOLT_SCALE" value="1 V" dimension="voltage"/>
        <Constant name="offset" value="0.010V" dimension="voltage"/>
        <Requirement name="alpha" dimension="per_time"/>
        <Requirement name="beta" dimension="per_time"/>
        <Dynamics>
            <DerivedVariable name="V" value="(v - offset) / VOLT_SCALE" dimension="none"/>
            <DerivedVariable name="ALPHA" value="alpha * TIME_SCALE" dimension="none"/>
            <DerivedVariable name="BETA" value="beta * TIME_SCALE" dimension="none"/>
            <ConditionalDerivedVariable exposure="t" name="t" dimension="time">
                <Case value="( 0 ) * TIME_SCALE" condition="(ALPHA + BETA) .eq. 0"/>
                <Case value="( 0.00005 ) * TIME_SCALE" condition="1/(ALPHA + BETA)  .lt. ( 0.00005 )"/>
                <Case value="( 1/(ALPHA + BETA)) * TIME_SCALE"/>
            </ConditionalDerivedVariable>
        </Dynamics>
    </ComponentType>
    <ComponentType extends="baseVoltageDepTime" name="Gran_NaF_98_h_tau_tau">
        <Constant name="TIME_SCALE" value="1 s" dimension="time"/>
        <Constant name="VOLT_SCALE" value="1 V" dimension="voltage"/>
        <Constant name="offset" value="0.010V" dimension="voltage"/>
        <Requirement name="alpha" dimension="per_time"/>
        <Requirement name="beta" dimension="per_time"/>
        <Dynamics>
            <DerivedVariable name="V" value="(v - offset) / VOLT_SCALE" dimension="none"/>
            <DerivedVariable name="ALPHA" value="alpha * TIME_SCALE" dimension="none"/>
            <DerivedVariable name="BETA" value="beta * TIME_SCALE" dimension="none"/>
            <ConditionalDerivedVariable exposure="t" name="t" dimension="time">
                <Case value="( 0 ) * TIME_SCALE" condition="(ALPHA + BETA) .eq. 0"/>
                <Case value="( 0.000225 ) * TIME_SCALE" condition="1/(ALPHA + BETA)  .lt. ( 0.000225 )"/>
                <Case value="( 1/(ALPHA + BETA)) * TIME_SCALE"/>
            </ConditionalDerivedVariable>
        </Dynamics>
    </ComponentType>
</neuroml>
