    </ionChannel>
    <ionChannel id="Gran_KCa_98" conductance="10pS" type="ionChannelHH" species="k">
        <notes>Calcium dependent K+ channel</notes>
        <annotation/>
        <gate id="m" instances="1" type="gateHHrates">
            <notes>Note: offset from ChannelML file incorporated into midpoint of rates!!</notes>
            <q10Settings experimentalTemp="17.350264793 degC" type="q10ExpTemp" q10Factor="3"/>
